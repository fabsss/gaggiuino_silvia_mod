    "jest-resolve": "^27.5.1",
    "jest-watch-typeahead": "^1.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.23",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.3",
    "prompts": "^2.4.2",
    "prop-types": "^15.8.1",
