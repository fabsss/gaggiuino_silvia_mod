    "react-app-polyfill": "^3.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-circular-input": "^0.2.4",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.11.2",
    "react-spring": "^9.7.1",
